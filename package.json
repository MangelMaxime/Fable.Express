{
  "name": "fable-express",
  "version": "0.0.0",
  "private": true,
  "description": "This repo is a meta repo for working on the binding for Express ecosystem",
  "author": "Mangel Maxime <mangel.maxime@protonmail.com>",
  "license": "MIT",
  "scripts": {
    "watch-test": "npx concurrently -n fable,nodemon \"npm run watch-test-fable\" \"npm run watch-test-nodemon\"",
    "watch-test-fable": "cd tests && dotnet fable Tests.fsproj --watch",
    "watch-test-nodemon": "npx nodemon --watch tests --exec 'mocha -r esm tests/Main.fs.js'"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "connect": "^3.7.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "mime": "^2.5.0",
    "qs": "^6.9.6",
    "range-parser": "^1.2.1",
    "serve-static": "^1.14.1",
    "supertest": "6.0.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "esm": "^3.2.25",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.7"
  }
}
